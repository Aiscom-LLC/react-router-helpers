{
  "name": "react-router-helpers",
  "version": "2.0.3",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "keywords": [
    "react",
    "react-router-dom"
  ],
  "license": "MIT",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/faker": "^4.1.10",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "concurrently": "^4.1.1",
    "cypress-intellij-reporter": "0.0.6",
    "eslint": "^6.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "prepend-file": "^1.3.1",
    "prettier": "^1.18.2",
    "rollup": "^1.19.4",
    "rollup-plugin-typescript2": "^0.22.1",
    "start-server-and-test": "^1.10.8",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "tslint-react": "^4.1.0",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "install:all": "npm i && cd example/example-for-test-only && npm i",
    "start:example-for-test": "cd example/example-for-test-only && npm start",
    "dev:example-for-test": "concurrently --kill-others \"npm run build:watch\" \"npm run start:example-for-test\"",
    "lint:example-for-test": "eslint ./src/**/*.ts ./src/**/*.tsx ./example/example-for-test-only/src/**/*.ts ./example/example-for-test-only/src/**/*.ts",
    "lint:example-for-test:fix": "npm run lint:example-for-test -- --fix",
    "deploy-example": "cd example && npm run deploy",
    "pretest": "npm run build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "react-router-dom": "^6.3.0"
  }
}
