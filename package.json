  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/faker": "^4.1.10",
    "@types/jest": "^27.5.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react-dom": "^18.0.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^4.1.18",
    "babel-core": "^6.26.3",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-dev-expression": "^0.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^4.1.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "prepend-file": "^1.3.1",
    "prettier": "^1.18.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^18.1.0",
    "rollup": "^1.19.4",
    "rollup-plugin-typescript2": "^0.22.1",
    "start-server-and-test": "^1.10.8",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "install:all": "npm i && cd example/for-development && npm i",
    "start:example-for-test": "cd example/for-development && npm start",
    "dev:example-for-test": "concurrently --kill-others \"npm run build:watch\" \"npm run start:example-for-test\"",
    "lint:example-for-test": "eslint ./src/**/*.ts ./src/**/*.tsx example/for-development/src/**/*.ts example/for-development/src/**/*.ts",
    "lint:example-for-test:fix": "npm run lint:example-for-test -- --fix",
    "deploy-example": "cd example && npm run deploy",
    "pretest": "npm run build",
    "test": "jest",
    "test:cy": "cypress run"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "react-router-dom": "^6.3.0"
  }
